      "home-product-focus": {
        "type": "home-product-focus",
        "disabled": true,
        "blocks": {
          "home-product-focus-3": {
            "type": "feature",
            "settings": {
              "title": "Accuracy",
              "text": "<p>The OneBite&nbsp;gives doctors the ability to capture the perfect record allowing you to have the peace of mind of complete accuracy.<\/p>",
              "icon": "locate"
            }
          },
          "home-product-focus-1": {
            "type": "feature",
            "settings": {
              "title": "Movements",
              "text": "<p>OneBite allows you to adjust both lateral and torque movements after the bite material has fully set giving&nbsp;the doctor the ability to&nbsp;fully capture the patients midline and horizontal line angles with no time restraint.<\/p>",
              "icon": "refresh"
            }
          },
          "home-product-focus-2": {
            "type": "feature",
            "settings": {
              "title": "Save Time",
              "text": "<p>Doctors only have to&nbsp;take one record with the OneBite verses three semi-accurate records with Benda brushes and q-tips. Saving&nbsp; both time and materials.<\/p>",
              "icon": "time"
            }
          },
          "home-product-focus-4": {
            "type": "feature",
            "settings": {
              "title": "Less Remakes",
              "text": "<p>The system provides highest quality communication to the lab which results in&nbsp;less costly repairs for the ceramist and doctor.<\/p>",
              "icon": "checkmark-circle-outline"
            }
          }
        },
        "block_order": [
          "home-product-focus-3",
          "home-product-focus-1",
          "home-product-focus-2",
          "home-product-focus-4"
        ],
